<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="100.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mar.tmlfx.view.controller.PlayerControllerVLCJ">
   <children>
      <Label prefHeight="18.0" prefWidth="75.0" text="Now playing:" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="5.0" />
      <Label fx:id="labelNowPlaying" text="-" AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="5.0" />
      <Slider fx:id="sliderPlayTime" layoutX="6.0" layoutY="29.0" prefHeight="14.0" prefWidth="584.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="10.0" />
      <Label fx:id="labelTime" layoutX="14.0" layoutY="28.0" text="Time" AnchorPane.leftAnchor="10.0" />
      <ButtonBar layoutY="50.0" prefHeight="50.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
        <buttons>
          <Button fx:id="buttonPlay" mnemonicParsing="false" onMousePressed="#handleClick" text="Play" />
            <Button fx:id="buttonStop" layoutX="535.0" layoutY="10.0" mnemonicParsing="false" onMousePressed="#handleClick" text="Stop" />
            <Button fx:id="buttonPrev" layoutX="535.0" layoutY="10.0" mnemonicParsing="false" onMousePressed="#handleClick" text="Prev" />
            <Button fx:id="buttonNext" layoutX="535.0" layoutY="10.0" mnemonicParsing="false" onMousePressed="#handleClick" text="Next" />
        </buttons>
         <padding>
            <Insets right="50.0" />
         </padding>
      </ButtonBar>
   </children>
</AnchorPane>
